    "@types/node": "^8.1.4",
    "@types/atom": "^1.23.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },
  "scripts": {
    "start": "tsc --watch"
  },
  "consumedServices": {
    "unibeautify": {
      "versions": {
        "^1.0.0": "consumeBeautifier"
      }
    }
  }
