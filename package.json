  "dependencies": {
    "bluebird": "^3.5.1",
    "unibeautify": "^0.2.0"
  },
  "activationCommands": {
    "atom-workspace": [
      "atom-beautify:help-debug-editor",
      "atom-beautify:beautify-editor",
      "atom-beautify:open-settings",
      "core:save",
      "core:save-as"
    ],
    ".tree-view .file .name": [
      "atom-beautify:beautify-file"
    ],
    ".tree-view .directory .name": [
      "atom-beautify:beautify-directory"
    ]
  },
  "keywords": [
    "atom",
    "unibeautify",
    "beautify",
    "beautifier",
    "format",
    "pretty",
    "clean"
